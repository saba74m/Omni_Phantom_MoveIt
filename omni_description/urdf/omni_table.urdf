<?xml version="1.0" encoding="utf-8"?>
<robot name="omni_phantom" xmlns:xacro="http://www.ros.org/wiki/xacro">
	
	<!-- and assemble your robot -->
	<!-- Attaching My Robot To the WORLD by introducing a dummy link-->
	<link name="world" />

    <joint name="world_to_box2" type="fixed">
      <origin xyz="0 2 0" rpy="0 0 0"/>
	    <parent link="world"/>
      <child link="box2"/>
    </joint>

	<link name="box2">
		<inertial>
			<mass value="2.0"/>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<inertia ixx="0.01"
             iyy="0.01"
             izz="0.01"
             ixy="0" iyz="0" ixz="0"/>
		</inertial>
		<visual>
			<origin xyz="0 0 0.50" rpy="0 0 0"/>
			<geometry>
				<box size="1.0 1.0 1.0"/>
			</geometry>
		</visual>
		<collision>
			<origin xyz="0 0 0.50" rpy="0 0 0"/>
			<geometry>
				<box size="1.0 1.0 1.0"/>
			</geometry>
		</collision>
	</link>

	<gazebo reference="box2">
		<static>true</static>
	</gazebo>

  <joint name="box2_to_base" type="fixed">
    <origin xyz="0 0 1" rpy="0 0 0"/>
	  <parent link="box2"/>
    <child link="base"/>
  </joint>

<!-- Materials -->
	<material name="metal_seamed">
		<color rgba="0.4627450980392157 0.4666666666666667 0.4509803921568628 1.0" />
	</material>
	<material name="blue">
		<color rgba="0.0 0.0 0.8 1.0" />
	</material>
	<material name="white">
		<color rgba="1.0 1.0 1.0 1.0" />
    </material>
	
	<!-- Links -->
  <link name="base" >
  	<inertial>
			<mass value="0.3"/>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<inertia ixx="0.01"
             iyy="0.01"
             izz="0.01"
             ixy="0" iyz="0" ixz="0"/>
		</inertial>
    <visual>
			<origin xyz="0 -0.02 0" />
      <geometry>
				<mesh filename="package://omni_description/meshes/base.stl" />
      </geometry>
      <material name="metal_seamed"/>
    </visual>
    <collision>
			<origin xyz="0 -0.02 0" />
      <geometry>
				<mesh filename="package://omni_description/meshes/base.stl" />
      </geometry>
      <material name="metal_seamed"/>
    </collision>
  </link>
	<!-- Link -->
  <link name="torso" >
  	<inertial>
			<mass value="0.2"/>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<inertia ixx="0.01"
             iyy="0.01"
             izz="0.01"
             ixy="0" iyz="0" ixz="0"/>
		</inertial>
    <visual>
			<origin xyz="0 0 0.036" rpy="-1.5707 0 0"/>
      <geometry>
				<mesh filename="package://omni_description/meshes/torso.stl" />
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
			<origin xyz="0 0 0.036" rpy="-1.5707 0 0"/>
      <geometry>
				<mesh filename="package://omni_description/meshes/torso.stl" />
      </geometry>
      <material name="blue"/>
    </collision>    
  </link>
	<!-- Link -->
  <link name="upper_arm" >
    <inertial>
			<mass value="0.1"/>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<inertia ixx="0.01"
             iyy="0.01"
             izz="0.01"
             ixy="0" iyz="0" ixz="0"/>
		</inertial>
		<visual>
			<origin xyz="0.0075 0 0" rpy="0 0 0"/>
      <geometry>
				<mesh filename="package://omni_description/meshes/upper_arm.stl" />
      </geometry>
      <material name="metal_seamed" />
    </visual>
		<collision>
			<origin xyz="0.0075 0 0" rpy="0 0 0"/>
      <geometry>
				<mesh filename="package://omni_description/meshes/upper_arm.stl" />
      </geometry>
      <material name="metal_seamed" />
    </collision>
  </link>
	<!-- Link -->
  <link name="lower_arm" >
    <inertial>
			<mass value="0.1"/>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<inertia ixx="0.01"
             iyy="0.01"
             izz="0.01"
             ixy="0" iyz="0" ixz="0"/>
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry>
				<mesh filename="package://omni_description/meshes/lower_arm.stl" />
      </geometry>
      <material name="metal_seamed" />
    </visual>
		<collision>
			<origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry>
				<mesh filename="package://omni_description/meshes/lower_arm.stl" />
      </geometry>
      <material name="metal_seamed" />
    </collision>
  </link>
	<!-- Link -->
  <link name="wrist" >
    <inertial>
			<mass value="0.2"/>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<inertia ixx="0.01"
             iyy="0.01"
             izz="0.01"
             ixy="0" iyz="0" ixz="0"/>
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="3.14159 0 0"/>
      <geometry>
				<mesh filename="package://omni_description/meshes/wrist.stl" />
      </geometry>
      <material name="metal_seamed" />
    </visual>
		<collision>
			<origin xyz="0 0 0" rpy="3.14159 0 0"/>
      <geometry>
				<mesh filename="package://omni_description/meshes/wrist.stl" />
      </geometry>
      <material name="metal_seamed" />
    </collision>
  </link>
	<!-- Link -->
  <link name="tip" >
    <inertial>
			<mass value="0.1"/>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<inertia ixx="0.01"
             iyy="0.01"
             izz="0.01"
             ixy="0" iyz="0" ixz="0"/>
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry>
				<mesh filename="package://omni_description/meshes/tip.stl" />
      </geometry>
      <material name="metal_seamed" />
    </visual>
		<collision>
			<origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry>
				<mesh filename="package://omni_description/meshes/tip.stl" />
      </geometry>
      <material name="metal_seamed" />
    </collision>
  </link>
  <!-- Link -->
  <link name="stylus" >
    <inertial>
			<mass value="0.1"/>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<inertia ixx="0.01"
             iyy="0.01"
             izz="0.01"
             ixy="0" iyz="0" ixz="0"/>
		</inertial>
    <visual>
			<origin xyz="0 0.013 0" rpy="1.5707 -1.5707 0"/>
      <geometry>
				<mesh filename="package://omni_description/meshes/pen.stl" />
      </geometry>
      <material name="blue" />
    </visual>
    <collision>
			<origin xyz="0 0.013 0" rpy="1.5707 -1.5707 0"/>
      <geometry>
				<mesh filename="package://omni_description/meshes/pen.stl" />
      </geometry>
      <material name="blue" />
    </collision>   
  </link>
  <!-- Link -->
  <link name="end_effector" >
  </link>
  
	<!-- joints -->
  <joint name="waist" type="revolute" >
    <parent link="base" />
    <child link="torso" />
    <origin xyz="0 0 0.09" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    <limit lower="-0.98" upper="0.98" velocity="100" effort="5" />
  </joint>
  <joint name="shoulder" type="revolute" >
    <parent link="torso" />
    <child link="upper_arm" />
    <origin xyz="-0.0075 0 0.035" rpy="0.23 0 0" />
    <axis xyz="1 0 0" />
    <limit lower="-0.24" upper="1.57" velocity="100" effort="5" />
  </joint>
  <joint name="elbow" type="revolute" >
    <parent link="upper_arm" />
    <child link="lower_arm" />
    <origin xyz="0.0075 .134 0" rpy="-2.15 0 0" />
    <axis xyz="1 0 0" />
		<!-- upper="1.25" -->
    <limit lower="-0.2" upper="1.95" velocity="100" effort="5" /> 
  </joint>
  <joint name="wrist1" type="revolute" >
    <parent link="lower_arm" />
    <child link="wrist" />
    <origin xyz="0 0.08 0" rpy="0 0 0"  />
    <axis xyz="0 1 0" />
    <limit lower="-2.54" upper="2.60" velocity="100" effort="5" />
  </joint>
  <joint name="wrist2" type="revolute" >
    <parent link="wrist" />
    <child link="tip" />
    <origin xyz="0 0.0525 0" rpy="-0.85 0 0" />
    <axis xyz="1 0 0" />
    <limit lower="-2.07" upper="0.23" velocity="100" effort="5" />
  </joint>
  <joint name="wrist3" type="revolute" >
    <parent link="tip" />
    <child link="stylus" />
    <origin xyz="0 -0.023 0" rpy="0 0 0" />
    <axis xyz="0 1 0" />
    <limit lower="-2.34" upper="2.34" velocity="100" effort="5" />
  </joint>
  <joint name="effector_transform" type="fixed">
      <parent link="tip" />
      <child link="end_effector" />
      <origin xyz="0 0.031 0" rpy="0 0 0" />
  </joint>

  <!-- Gazebo Configurations -->
  <transmission name="trans_waist">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="waist">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="waist_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
  </transmission>
  <transmission name="trans_shoulder">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="shoulder">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="shoulder_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
  </transmission>
  <transmission name="trans_elbow">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="elbow">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="elbow_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
  </transmission>
  <transmission name="trans_wrist1">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="wrist1">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="wrist1_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
  </transmission>
  <transmission name="trans_wrist2">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="wrist2">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="wrist2_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
  </transmission>
  <transmission name="trans_wrist3">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="wrist3">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="wrist3_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
  </transmission>

	<gazebo reference="base">
			<gravity>true</gravity>
			<material>Gazebo/Metal_seamed</material>
		</gazebo>

	<gazebo reference="torso">
			<gravity>true</gravity>
			<material>Gazebo/Blue</material>
		</gazebo>

	<gazebo reference="upper_arm">
			<gravity>true</gravity>
			<material>Gazebo/Metal_seamed</material>
		</gazebo>

	<gazebo reference="lower_arm">
			<gravity>true</gravity>
			<material>Gazebo/Metal_seamed</material>
		</gazebo>

	<gazebo reference="wrist">
			<gravity>true</gravity>
			<material>Gazebo/Metal_seamed</material>
		</gazebo>

	<gazebo reference="tip">
			<gravity>true</gravity>
			<material>Gazebo/Metal_seamed</material>
		</gazebo>

	<gazebo reference="end_effector">
			<gravity>true</gravity>
			<material>Gazebo/Metal_seamed</material>
		</gazebo>

	<gazebo reference="stylus">
			<gravity>true</gravity>
			<material>Gazebo/Blue</material>
		</gazebo>

  <gazebo>
        <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
            <robotNamespace>/phantom_omni</robotNamespace>
        </plugin>
  </gazebo>

</robot>

